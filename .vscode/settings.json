{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/Library": true,
        "**/Temp": true,
        "**/obj": true,
        "**/Build": true,
        "**/Builds": true,
        "**/*.meta": true
    },
    "files.associations": {
        "*.asset": "yaml",
        "*.meta": "yaml",
        "*.prefab": "yaml",
        "*.unity": "yaml",
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.sln": "*.csproj",
    },
    "dotnet.defaultSolution": "DiceStorm.sln"
}